<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
  "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>items</key>
  <array>
    <dict>

      <!-- 📦 The .ipa file download URL (must be HTTPS) -->
      <key>assets</key>
      <array>
        <dict>
          <key>kind</key>
          <string>software-package</string>
          <key>url</key>
          <string>https://theshubh.github.io/app-dist/lkp_crm_mobile.ipa</string>
          <!-- 👆 Replace this with the direct HTTPS link to your signed .ipa file -->
        </dict>
      </array>

      <!-- 📝 App metadata -->
      <key>metadata</key>
      <dict>

        <!-- 📱 Your app’s bundle identifier -->
        <key>bundle-identifier</key>
        <string>com.livekeeping.crm</string>
        <!-- 👆 Replace with your app's bundle ID from Xcode (must match the provisioning profile) -->

        <!-- 🔢 App version (display only) -->
        <key>bundle-version</key>
        <string>1.0.3</string>
        <!-- 👆 Replace with your app version -->

        <key>kind</key>
        <string>software</string>

        <!-- 📛 App display name shown during installation -->
        <key>title</key>
        <string>Livekeeping CRM</string>
        <!-- 👆 This is what users see when prompted to install the app -->

      </dict>

    </dict>
  </array>
</dict>
</plist>
